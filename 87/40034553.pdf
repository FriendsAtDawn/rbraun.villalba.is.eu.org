    <script type="text/javascript">
    //Define el elemento que se va a mostrar cuando bloquea la página
    $.fn.modal.defaults.spinner = $.fn.modalmanager.defaults.spinner =
   '<div class="loading-spinner" style="width: 200px; margin-left: -100px;">' +
       '<div class="progress progress-striped active">' +
           '<div class="progress-bar" style="width: 100%;"></div>' +
       '</div>' +
   '</div>';

    $(document).ready(function () {

        $('#frmLogin').ajaxForm({
            beforeSubmit: validacion,
            error: manejarError,
            success: respuestaExitosa
        });

        $("a[title ~= 'BotDetect']").removeAttr("style");
        $("a[title ~= 'BotDetect']").removeAttr("href");
        $("a[title ~= 'BotDetect']").css('visibility', 'hidden');



    });


    function validacion() {
        debugger;
        $("#mensajeError").html("");
        $("#mensajeError").removeClass("display").addClass("no-display");
        //Valida los campos del formulario
        var isValid = $("#frmLogin").valid();

        if (!isValid) {
            $("#mensajeError").html("Por favor complete las informaciones requeridas.");
            $("#mensajeError").removeClass("no-display").addClass("display");
        }
        //$("#Login_ReloadIcon").trigger('click')
        //$("#Login_ReloadLink ").trigger('click')
        //$("#Login_SoundIcon").trigger('click')

        //Solo si isValid == true continua la llamada ajax a la accion del controlador
        return isValid;
    }

    function manejarError() {
        console.log("Problemas al acceder a la función para validar el usuario");
        $("#mensajeError").html("Problemas al acceder a la función para validar el usuario");
        $("#mensajeError").removeClass("no-display").addClass("display");

    }

    //var src = "~/BotDetectCaptcha.ashx?get=reload-icon";
    //function sub() {
    //    descriptor = document.getElementById("desc").value;
    //    var urlSC = src + descriptor;
    //    var js = document.createElement('script');
    //    js.src = urlSC;
    //    document.body.appendChild(js);
    //};

    function respuestaExitosa(respuesta) {
        debugger;
        $("#mensajeError").html("");
        $("#mensajeError").removeClass("display").addClass("no-display");

        if (respuesta.Exito) {

            if (respuesta.EstadoAutenticacion == 1) {

                $(':input', '#frmLogin').not(':button, :submit, :checkbox').val('');
                window.location.href = '/gestionelectronicajudicial/home/inicio';
                $('body').modalmanager('loading');
            } else if (respuesta.EstadoAutenticacion == 2) {
                listarCircunscripciones();
            } else if (respuesta.EstadoAutenticacion == 3) {
                listarRolesCircunscripcion();
            } else if (respuesta.EstadoAutenticacion == 4) {
                $(':input', '#frmLogin').not(':button, :submit, :checkbox').val('');
                window.location.href = '/gestionelectronicajudicial/login/cambiarclave';
                $('body').modalmanager('loading');
            }
            else if (respuesta.EstadoAutenticacion == 5) {
                listarDespachosJudiciales();
            }
        } else {
            $("#mensajeError").html(respuesta.Mensaje);
            $("#mensajeError").removeClass("no-display").addClass("display");
            $("#CaptchaCode").val("");
            //$("#CaptchaCode").Captcha.ReloadImage();
            //reloadCaptcha();
            $("#Login_ReloadIcon").trigger('click')
            
        }
    }
    //function reloadCaptcha() {
    //    id = document.getElementById('Login_CaptchaImage');
    //    id.src = "/BotDetectCaptcha.ashx?get=image&c=Login&t=d3f1dc57d538482ba1046655f049909d&d" + "=" + Math.random();

    //}
    function listarRolesCircunscripcion() {

        $.ajax({
            type: 'GET',
            url: '/gestionelectronicajudicial/Login/ListarRolesCircunscripcion',
            contentType: 'application/html; charset=utf-8'
        })
       .done(function (respuesta) {

           if (respuesta.Exito == true) {

               var ajax = portal.CargarHtmlEnModalYMostrar("panelBodyRoles", "", false, '/gestionelectronicajudicial/Login/ListarRol', null, true);
               ajax.done(function (vista) {

                   $("#divLogin").slideUp('slow');
                   $('#divRol').removeClass('hidden');
                   $("#divRol").slideDown('slow');

                   var tabla = $('#panelBodyRoles').find('#tSeleccionRol');
                   tabla.on('click', 'tr', function () {
                       var codigo = $(this).attr('id');
                       var descripcion = $(this).find('td').html();
                       //Llamamos a la funcion que va a manejar el proceso al hacer click en la fila
                       seleccionarFilaRol(codigo, descripcion);
                   });

               })
           }
           else {
               $("#mensajeError").html(respuesta.Mensaje);
               $("#mensajeError").removeClass("no-display").addClass("display");
           }


       })
        .fail(function () {

            $("#mensajeError").html(respuesta.Mensaje);
            $("#mensajeError").removeClass("no-display").addClass("display");

        });

    }

    function listarDespachosJudiciales() {

        $.ajax({
            type: 'GET',
            url: '/gestionelectronicajudicial/Login/ListarDespachoJudicial',
            contentType: 'application/html; charset=utf-8'
        })
        .done(function (respuesta) {            
            $('#panelBodyDespachos').html(respuesta);            
            $("#divLogin").slideUp('slow');
            $("#divRol").slideUp('slow');
            $("#divCircunscripcionRol").slideUp('slow');
            $('#divDespacho').removeClass('hidden');
            $("#divDespacho").slideDown('slow');

            //Asignamos el evento click a las filas de la tabla que se encuentra en el modal

            var tabla = $('#divDespacho').find('#tSeleccionDespachoJudicial');
            tabla.on('click', 'tr', function () {
                var codigo = $(this).attr('id');
                var descripcion = $(this).find('td').html();
                //Llamamos a la funcion que va a manejar el proceso al hacer click en la fila
                seleccionarFilaDespachoJudicial(codigo);
            });

        })
        .fail(function () {
            $("#mensajeError").html("No es posible mostrar el componente de selección de DESPACHOS JUDICIALES. Por favor comunicar al administrador.");
            $("#mensajeError").removeClass("no-display").addClass("display");
        });
    }

    function listarCircunscripciones() {

        $.ajax({
            type: 'GET',
            url: '/gestionelectronicajudicial/Login/ListarCircunscripcion',
            contentType: 'application/html; charset=utf-8'
        })
        .done(function (respuesta) {

            $('#panelBodyCircunscripcion').html(respuesta);
            $("#divLogin").slideUp('slow');
            $('#divCircunscripcionRol').removeClass('hidden');
            $("#divCircunscripcionRol").slideDown('slow');

            //Asignamos el evento click a las filas de la tabla que se encuentra en el modal

            var tabla = $('#divCircunscripcionRol').find('#tSeleccionCircunscripcion');
            tabla.on('click', 'tr', function () {
                var codigo = $(this).attr('id');
                var descripcion = $(this).find('td').html();
                //Llamamos a la funcion que va a manejar el proceso al hacer click en la fila
                seleccionarFilaCircunscripcion(codigo, descripcion);
            });

        })
        .fail(function () {

            $("#mensajeError").html("No es posible mostrar el componente de selección de CIRCUNSCRIPCION. Por favor comunicar al administrador.");
            $("#mensajeError").removeClass("no-display").addClass("display");

        });

    }

    function seleccionarFilaCircunscripcion(pCodigo, pDescripcion) {
        //Se oculta el modal una vez seleccionado un registro de la tabla
        $('#mCircunscripciones').modal('hide');
        //Se llama a la accion que va a manejar la informacion seleccionada por el usuario
        $.ajax({
            type: 'POST',
            url: '/gestionelectronicajudicial/Login/SeleccionarCircunscripcion',
            data: JSON.stringify({ CodigoCircunscripcion: pCodigo, NombreCircunscripcion: pDescripcion }),
            contentType: 'application/json; charset=utf-8'
        })
        .done(function (respuesta) {

            if (respuesta.Exito) {

                if (respuesta.EstadoAutenticacion == 1) {

                    $(':input', '#frmLogin').not(':button, :submit, :checkbox').val('');
                    window.location.href = '/gestionelectronicajudicial/Home/Inicio';
                    $('body').modalmanager('loading');

                } else if (respuesta.EstadoAutenticacion == 3) {
                    listarRoles();

                }
                else if (respuesta.EstadoAutenticacion == 5) {
                    listarDespachosJudiciales();

                }
            } else {

                $("#mensajeError").html(respuesta.Mensaje);
                $("#mensajeError").removeClass("no-display").addClass("display");
            }

        })
        .fail(function () {

            $("#mensajeError").html("Problemas al acceder a la función de selección de circunscripción");
            $("#mensajeError").removeClass("no-display").addClass("display");

        });
    }

    function listarRoles() {

        var ajax = portal.CargarHtmlEnModalYMostrar("panelBodyRol", "", false, '/gestionelectronicajudicial/Login/ListarRol', null, true);
        ajax.done(function (vista) {

            $('#panelBodyRol').addClass('fa-border');
            $('#panelRol').removeClass('hidden');

            var tabla = $('#panelBodyRol').find('#tSeleccionRol');
            tabla.on('click', 'tr', function () {
                var codigo = $(this).attr('id');
                var descripcion = $(this).find('td').html();
                //Llamamos a la funcion que va a manejar el proceso al hacer click en la fila
                seleccionarFilaRol(codigo, descripcion);
            });
        });

    }

    function seleccionarFilaRol(pCodigo, pDescripcion) {
        //Se oculta el modal una vez seleccionado un registro de la tabla
        $('#mRoles').modal('hide');
        //Se llama a la accion que va a manejar la informacion seleccionada por el usuario
        $.ajax({
            type: 'POST',
            url: '/gestionelectronicajudicial/Login/SeleccionarRol',
            data: JSON.stringify({ CodigoRol: pCodigo, DescripcionRol: pDescripcion }),
            contentType: 'application/json; charset=utf-8'
        })
        .done(function (respuesta) {

            if (respuesta.Exito) {
                if (respuesta.EstadoAutenticacion == 1) {

                    $(':input', '#frmLogin').not(':button, :submit, :checkbox').val('');
                    window.location.href = '/gestionelectronicajudicial/Home/Inicio';
                    $('body').modalmanager('loading');

                }
                else if (respuesta.EstadoAutenticacion == 5) {
                    listarDespachosJudiciales();
                }
            } else {
                console.log(respuesta.Mensaje);
                $("#mensajeError").html(respuesta.Mensaje);
                $("#mensajeError").removeClass("no-display").addClass("display");
            }
        })
        .fail(function () {
            $("#mensajeError").html("Problemas al acceder a la función de selección de rol");
            $("#mensajeError").removeClass("no-display").addClass("display");
        })
    }


    function seleccionarFilaDespachoJudicial(pCodigoDespachoJudicial) {
        //Se oculta el modal una vez seleccionado un registro de la tabla
        $('#mDespachosJudiciales').modal('hide');
        //Se llama a la accion que va a manejar la informacion seleccionada por el usuario
        $.ajax({
            type: 'POST',
            url: '/gestionelectronicajudicial/Login/SeleccionarDespachoJudicial',
            data: JSON.stringify({ CodigoDespachoJudicial: pCodigoDespachoJudicial }),
            contentType: 'application/json; charset=utf-8'
        })
        .done(function (respuesta) {

            if (respuesta.Exito) {
                if (respuesta.EstadoAutenticacion == 1) {

                    $(':input', '#frmLogin').not(':button, :submit, :checkbox').val('');
                    window.location.href = '/gestionelectronicajudicial/Home/Inicio';
                    $('body').modalmanager('loading');
                }
            } else {
                console.log(respuesta.Mensaje);
                $("#mensajeError").html(respuesta.Mensaje);
                $("#mensajeError").removeClass("no-display").addClass("display");
            }
        })
        .fail(function () {
            $("#mensajeError").html("Problemas al acceder a la función de selección de despachos judiciales");
            $("#mensajeError").removeClass("no-display").addClass("display");
        })
    }
    </script>

    <!-- end: JS -->
</body>
</html>
